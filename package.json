{
  "name": "js-in-css",
  "version": "0.0.0",
  "description": "Need to write CSS, but tired of SASS, LESS et.al? Try JavaScript/TypeScript in CSS.",
  "main": "index.js",
  "scripts": {
    "format": "eslint --fix  \"*.{js,ts,tsx}\" \"src/**/*.{js,ts,tsx}\"  &&  prettier --write \"*.{json,md}\"  \"src/**/*.{json,md}\""
  },
  "repository": "git@github.com:maranomynet/js-in-css.git",
  "author": "Már Örlygsson",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@hugsmidjan/hxmstyle": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "esbuild": "^0.14.18",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-destructure-depth": "^1.0.3",
    "eslint-plugin-destructuring": "^2.2.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-total-functions": "^5.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "ospec": "^4.1.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "hxmstyle": {
    "options": {
      "typescript": true
    },
    "dependenciesAdded": [
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "eslint",
      "eslint-config-prettier",
      "eslint-plugin-deprecation",
      "eslint-plugin-destructure-depth",
      "eslint-plugin-destructuring",
      "eslint-plugin-import",
      "eslint-plugin-prettier",
      "eslint-plugin-simple-import-sort",
      "eslint-plugin-total-functions",
      "eslint-plugin-unused-imports",
      "prettier"
    ]
  }
}
