{
  "name": "es-in-css",
  "version": "0.3.2",
  "description": "Need to write CSS, but tired of SASS, LESS et al? Try JavaScript/TypeScript in CSS.",
  "scripts": {
    "format": "eslint --fix  \"*.{js,ts,tsx}\" \"src/**/*.{js,ts,tsx}\"  &&  prettier --write \"*.{json,md}\"  \"src/**/*.{json,md}\"",
    "build": "yarn install  &&  tsc --noEmit  &&  node build.js  &&  yarn run test",
    "dev": "yarn install  &&  yarn run concurrently \"tsc --noEmit --watch --preserveWatchOutput\"  \"node build.js --dev\"  \"yarn run test:dev\"",
    "prepublishOnly": "echo \"\nRun 'yarn run publishdist' to publish this package\n\n\"  && exit 1",
    "publishdist": "yarn run build  &&  cd dist  &&  npm publish",
    "publishcanary": "yarn run build  &&  cd dist  &&  npm publish --tag canary",
    "test:dev": "sleep 1  &&  (yarn run test || true)  &&  yarn run test:watch",
    "test:watch": "yarn run onchange '__tests/**/*.tests-*.js' -f add -f change -- yarn run ospec '{{file}}'",
    "test:cli": "node dist/bin/cli.js \"__tests/**/*.css.js\"",
    "test": "yarn run ospec \"__tests/**/*.tests-*.js\"",
    "es-in-css": "node ./dist/bin/cli.js"
  },
  "repository": "git@github.com:maranomynet/es-in-css.git",
  "author": "Már Örlygsson",
  "contributors": [
    "Ragnheiður Ásta Karlsdóttir <ragnheidur.asta.karlsdottir@reykjavik.is>",
    "Már Örlygsson <mar.orlygsson@anomy.net>"
  ],
  "license": "MIT",
  "private": true,
  "dist_package_json": {
    "engines": {
      "node": ">=14"
    },
    "main": "index.js",
    "module": "index.mjs",
    "types": "index.d.ts",
    "bin": {
      "es-in-css": "bin/cli.js"
    }
  },
  "engines": {
    "node": "^16"
  },
  "dependencies": {
    "@types/color": "^3.0.3",
    "@types/color-name": "^1.1.1",
    "@types/css-prettier": "npm:@types/prettier@^2.4.4",
    "autoprefixer": "^10.4.2",
    "color": "^4.2.1",
    "color-name": "^1.1.4",
    "commander": "^9.0.0",
    "css-prettier": "npm:prettier@^2.6.0",
    "cssnano": "^5.1.0",
    "glob": "^7.2.0",
    "postcss": "^8.4.6",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^4.0.3"
  },
  "devDependencies": {
    "@hugsmidjan/hxmstyle": "^6.2.0",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.17",
    "@types/ospec": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "concurrently": "^7.0.0",
    "esbuild": "^0.14.18",
    "esbuild-plugin-d.ts": "^1.1.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-destructure-depth": "^1.0.3",
    "eslint-plugin-destructuring": "^2.2.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-total-functions": "^5.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "minimist": "^1.2.6",
    "onchange": "^7.1.0",
    "ospec": "^4.1.1",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2"
  },
  "hxmstyle": {
    "options": {
      "typescript": true
    },
    "dependenciesAdded": [
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "eslint",
      "eslint-config-prettier",
      "eslint-plugin-deprecation",
      "eslint-plugin-destructure-depth",
      "eslint-plugin-destructuring",
      "eslint-plugin-import",
      "eslint-plugin-prettier",
      "eslint-plugin-simple-import-sort",
      "eslint-plugin-total-functions",
      "eslint-plugin-unused-imports",
      "prettier"
    ]
  },
  "packageManager": "yarn@3.2.0"
}
